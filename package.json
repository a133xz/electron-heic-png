{
  "name": "electron-heic-png",
  "description": "Electron program to convert HEIC to PNG",
  "version": "1.0.0 ",
  "author": {
    "name": "Antonio R"
  },
  "keywords": [
    "electron",
    "electron vuejs parcel",
    "parcel",
    "vuejs",
    "heic to png"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/a133xz/electron-heic-png.git"
  },
  "license": "MIT",
  "main": "src/electron.js",
  "scripts": {
    "vue-serve": "parcel ./src/index.html -p 3000 --dist-dir build",
    "vue-build": "parcel build ./src/index.html --dist-dir build --public-url ./",
    "serve": "concurrently \"cross-env BROWSER=none yarn vue-serve\" \"wait-on http://localhost:3000 && electron . \"",
    "clean-build": "rm -rf build/ .cache dist/",
    "electron-build": "electron-builder -mwl",
    "build:local": "yarn clean-build && yarn vue-build && yarn electron-build",
    "build": "yarn vue-build",
    "patch": "yarn changelog && npm version patch -m \"v%s\"",
    "changelog": "github_changelog_generator -u a133xz -p electron-heic-png"
  },
  "dependencies": {
    "electron-is-dev": "^1.2.0",
    "heic-convert": "^1.2.4",
    "parcel": "^2.0.0-beta.3.1",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.0.0-nightly.724",
    "@parcel/transformer-vue": "2.0.0-nightly.2346",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "electron": "^13.1.2",
    "electron-builder": "^22.11.7",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.0.0",
    "prettier": "^2.3.1",
    "sass": "^1.35.1",
    "typescript": "^4.3.2",
    "wait-on": "^5.0.0"
  },
  "build": {
    "appId": "com.electron.electron-vuejs-parcel",
    "productName": "MyApp",
    "files": [
      "src/electron.js",
      "src/assets/*",
      "build/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "src/icons/icon.icns"
    },
    "win": {
      "icon": "src/icons/icon.ico"
    },
    "linux": {
      "icon": "src/icons"
    }
  },
  "bundlesize": [
    {
      "path": "./build/src.*.js",
      "maxSize": "250 kB"
    }
  ],
  "homepage": "./"
}